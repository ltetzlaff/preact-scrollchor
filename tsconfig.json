{
    "compileOnSave": true,
    "compilerOptions": {
      "target": "es5",
      "sourceMap": true,
      "declaration": true,
      "strict": true,
      "lib": [
        "esnext", "dom"
      ],
      "jsx": "react",
      "jsxFactory": "h",
      "outDir": "lib",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "include": [
        "src/**/*"
    ]
}
